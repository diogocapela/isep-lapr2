<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>
<BorderPane xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="lapr.project.ui.UC30ShowEventAcceptanceRateUI">
    <center>
        <HBox alignment="CENTER" prefWidth="-1.0" BorderPane.alignment="CENTER">
            <children>
                <VBox alignment="CENTER">
                    <children>
                        <TableView fx:id="eventTable" prefWidth="-1.0">
                            <columns>
                                <TableColumn fx:id="eventColTitle" minWidth="150.0" prefWidth="-1.0" />
                                <TableColumn fx:id="eventColDescription" minWidth="300.0" prefWidth="-1.0" />
                            </columns>
                            <VBox.margin>
                                <Insets />
                            </VBox.margin>
                        </TableView>
                    </children>
                </VBox>
            </children>
        </HBox>
    </center>
   <bottom>
      <VBox alignment="CENTER" BorderPane.alignment="CENTER">
         <children>
                  <Separator>
               <VBox.margin>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
               </VBox.margin>
                  </Separator>
                  <Button mnemonicParsing="false" onAction="#handleGetButtonEvent" text="Get Event's Acceptance Rate">
               <VBox.margin>
                  <Insets bottom="2.0" left="2.0" right="2.0" top="2.0" />
               </VBox.margin>
            </Button>
                  <Label fx:id="labelAcceptanceRate">
                      <VBox.margin>
                          <Insets bottom="2.0" left="2.0" right="2.0" top="2.0" />
                      </VBox.margin>
                  </Label>
         </children>
      </VBox>
   </bottom>
   <top>
      <Label text="UC30 - Show Event's Acceptance Rate" BorderPane.alignment="CENTER">
         <font>
            <Font name="System Bold" size="20.0" />
         </font>
      </Label>
   </top>
</BorderPane>
